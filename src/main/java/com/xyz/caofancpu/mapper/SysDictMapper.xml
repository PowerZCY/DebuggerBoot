<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xyz.caofancpu.mapper.SysDictMapper">

    <select id="getInitSysDictList" resultType="java.util.Map">
        SELECT name, code FROM sys_dict WHERE group_id = 0 ORDER BY sort_id ASC
    </select>

    <select id="getSysDictList" resultType="java.util.Map">
        SELECT name, code FROM sys_dict WHERE 1 = 1 ORDER BY sort_id ASC
    </select>


</mapper>

